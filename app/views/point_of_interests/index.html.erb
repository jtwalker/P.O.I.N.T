<h1>Listing All POIs</h1>
 
<table>
  <tr>
    <th>User Id</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
  <% @pois.each do |poi| %>
    <% if poi.user_id == current_user.id %>
      <tr>
        <td><%= poi.user_id %></td>
        <td><%= poi.latitude %></td>
        <td><%= poi.longitude %></td>
        <td><%= link_to 'Show', poi %></td>
        <td><%= link_to 'Edit', edit_point_of_interest_path(poi) %></td>
        <td><%= link_to 'Destroy', point_of_interest_path(poi), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  <% end %>
</table>